{
  "name": "@redactie/views-module",
  "version": "1.4.1",
  "description": "redactie views module",
  "main": "dist/redactie-views-module.umd.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/redactie-views-module.umd.js",
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "webpack --env.prod && npm run build:types",
    "build:w": "webpack -w --env.prod && npm run build:types",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "typedoc --plugin none --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --out docs",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme --entryPoints ./public/index.tsx --entryPoints ./public/lib/api/index.ts --excludePrivate --theme github-wiki --out docs/markdown"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.antwerpen.be/reda/redactie-views_module_react.git"
  },
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "keywords": [
    "Redactie",
    "React",
    "Views",
    "Module"
  ],
  "devDependencies": {
    "@acpaas-ui/react-components": "3.4.1",
    "@acpaas-ui/react-editorial-components": "0.0.3",
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@datorama/akita": "4.22.1",
    "@redactie/content-module": "^0.22.8",
    "@redactie/form-renderer-module": "^1.8.10",
    "@redactie/react-router-guards": "1.0.2",
    "@redactie/redactie-core": "2.0.0",
    "@redactie/roles-rights-module": "0.10.0",
    "@redactie/translations-module": "0.8.3",
    "@redactie/utils": "2.1.0",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.9.1",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.kebabcase": "4.1.6",
    "@types/ramda": "0.27.32",
    "@types/react": "16.9.19",
    "@types/react-router-dom": "5.1.3",
    "@types/yup": "0.26.33",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "babel-jest": "25.1.0",
    "concurrently": "5.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "formik": "2.1.4",
    "husky": "3.1.0",
    "jest": "25.1.0",
    "ky": "0.17.0",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "ramda": "0.27.0",
    "react": "16.12.0",
    "react-router-dom": "5.1.2",
    "stylelint": "13.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-jest": "25.2.1",
    "typescript": "4.6.3",
    "typedoc": "^0.22.4",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript-plugin-css-modules": "2.1.2",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "yup": "0.28.3"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "4.1.1",
    "query-string": "^6.12.1",
    "uuid": "^8.3.1"
  },
  "author": "Maarten De Weerdt",
  "license": "ISC"
}
