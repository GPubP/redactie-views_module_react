{
  "name": "@redactie/views-module",
  "version": "0.17.3",
  "description": "redactie views module",
  "main": "dist/redactie-views-module.umd.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/redactie-views-module.umd.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && webpack --env.prod",
    "build:w": "npm run build:types && webpack -w --env.prod",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.antwerpen.be/reda/redactie-views_module_react.git"
  },
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
